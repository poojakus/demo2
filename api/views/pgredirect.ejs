<html>
<head>
<title>Merchant Check Out Page</title>
</head>
<body>
	<% if (status == 1) { %>
	<center><h1>Please do not refresh this page...</h1></center><br>
	<center>
		<img src="../images/loading.gif" alt="">
	</center>

		<form method="post" action="<%= redirect_url%>" name="f1"> 
		<table border="1">
			<tbody>
			<% for (name in restdata) { 
				console.log(name); 
				console.log(restdata[name] ); 
			%>
				<input type="hidden" name='<%= name %>' value='<%= restdata[name] %>'>

			<%}%>
			</tbody>
		</table>
		<script type="text/javascript">
			document.f1.submit();
		</script>
	</form>
	<% } else { %>
	<center><h1 style="color:red"><%= msg %></h1></center>	
	<% } %>	

</body>
</html>