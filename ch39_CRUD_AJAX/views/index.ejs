<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">\
    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <title>Document</title>
</head>
<body>
    <h1>CRUD FORM</h1>
    <form id="person" >
        Name:-<input type="text" name="name" id="name" placeholder="Enter Name"><br>
        Email:-<input type="email" name="email" id="email" placeholder="Enter Email"><br>
        Password:-<input type="password" name="password" placeholder="ENter Password"><br>
        <input type="submit" id="submit" value="submit" >


    </form>
    <h1>THis is pooja</h1>
    <button id="bt">Click here</button>
    <script>
        $(document).ready(function()
        {
           $("#person").on('submit',function(event)
           {
                event.preventDefault();
                const personData=$(this).serialize();
                console.log(personData)

                $.ajax({
                    url:'/create',
                    method:'post',
                    data:personData,
                    success:function(result,status,xhr)
                    {
                        console.log("Successfully.....")
                        console.log(result)
                    },
                    error: function(xhr,status,error)
                    {
                        console.log('somethinf went wrong')
                    }

                });
           });
        });
    </script>
</body>
</html>